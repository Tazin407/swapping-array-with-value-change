#include<stdio.h>

int main(){
      int n;
      int x;
      int v;
      scanf("%d ", &n);
      int arr[n];
      for (int  i = 0; i < n; i++)
      {
        scanf("%d", &arr[i]);
      }
      scanf("%d %d", &x, &v);
      arr[x]=v;
      for (int i =1; i<=n; i++)
      {
        printf("%d ", arr[n-i]);
        
      }
      


    return 0;
}
